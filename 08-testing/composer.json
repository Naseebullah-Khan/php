{
  "name": "naseeb_khan/01-composer-and-free-autoloading",
  "authors": [
    {
      "name": "Naseeb Khan",
      "email": "110621136+Naseebullah-Khan@users.noreply.github.com"
    }
  ],
  "require": {
    "illuminate/collections": "^12.52"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "Http\\": "Http/",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^4.4"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
